@{
    ViewData["Title"] = "Principal page";
}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Información de Condominio</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
            margin-top: 20px;
        }

        th, td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>

    <h2>Información de Condominio</h2>

    <table>
        <thead>
            <tr>
                <th>CondominioID</th>
                <th>Código</th>
                <th>Nombre</th>
                <th>Dirección</th>
                <th>Teléfono Oficina</th>
            </tr>
        </thead>
        <tbody>
            <!-- Aquí puedes insertar filas con información de condominios -->
            <tr>
                <td>1</td>
                <td>ABC123</td>
                <td>Condominio Ejemplo</td>
                <td>Calle 123, Ciudad</td>
                <td>123-456-7890</td>
            </tr>
            <!-- Puedes agregar más filas según sea necesario -->
        </tbody>
    </table>

</body>
</html>
